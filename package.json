{
  "name": "generator-create-service",
  "version": "1.9.0",
  "description": "Boring Express Microservice Generator",
  "homepage": "https://github.com/boringcodes/create-service",
  "bugs": "https://github.com/boringcodes/create-service/issues",
  "license": "MIT",
  "author": "BoringCodes <hi@boringcodes.com> (https://boringcodes.com)",
  "repository": "https://github.com/boringcodes/create-service.git",
  "main": "generators/app/index.js",
  "files": [
    "generators"
  ],
  "scripts": {
    "purge": "rm -rf node_modules",
    "format": "prettier --write --ignore-path .gitignore **/*.js",
    "format:check": "prettier --check --ignore-path .gitignore **/*.js",
    "lint": "eslint --ignore-path .gitignore --ignore-pattern templates **/*.js",
    "release": "standard-version --no-verify",
    "release:major": "yarn release --release-as major",
    "release:minor": "yarn release --release-as minor",
    "release:patch": "yarn release --release-as patch"
  },
  "keywords": [
    "yeoman-generator",
    "create-service"
  ],
  "dependencies": {
    "chalk": "^3.0.0",
    "change-case": "^4.1.1",
    "yeoman-generator": "^4.5.0",
    "yosay": "^2.0.2"
  },
  "devDependencies": {
    "@boringcodes/eslint-config": "^1.0.0",
    "@boringcodes/prettier-config": "^1.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.9",
    "prettier": "^2.0.2",
    "standard-version": "^7.1.0"
  }
}
